{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://annolid.org/schemas/agent_output.schema.json",
  "title": "Annolid Frame NDJSON Record",
  "type": "object",
  "required": [
    "version",
    "video_name",
    "frame_index",
    "imagePath",
    "imageHeight",
    "imageWidth",
    "shapes",
    "flags",
    "otherData"
  ],
  "properties": {
    "version": {
      "type": "string",
      "minLength": 1
    },
    "video_name": {
      "type": "string",
      "minLength": 1
    },
    "frame_index": {
      "type": "integer",
      "minimum": 0
    },
    "timestamp_sec": {
      "type": "number",
      "minimum": 0
    },
    "imagePath": {
      "type": "string"
    },
    "imageHeight": {
      "type": "integer",
      "minimum": 1
    },
    "imageWidth": {
      "type": "integer",
      "minimum": 1
    },
    "flags": {
      "type": "object",
      "additionalProperties": true
    },
    "otherData": {
      "type": "object",
      "additionalProperties": true
    },
    "shapes": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/shape"
      }
    }
  },
  "definitions": {
    "shape": {
      "type": "object",
      "required": [
        "label",
        "points",
        "shape_type"
      ],
      "properties": {
        "label": {
          "type": "string"
        },
        "points": {
          "type": "array",
          "items": {
            "type": "array",
            "items": {
              "type": "number"
            },
            "minItems": 2,
            "maxItems": 2
          }
        },
        "shape_type": {
          "type": "string",
          "minLength": 1
        },
        "group_id": {
          "type": [
            "integer",
            "null"
          ]
        },
        "flags": {
          "type": "object",
          "additionalProperties": true
        },
        "visible": {
          "type": "boolean"
        },
        "description": {
          "type": "string"
        },
        "mask": {
          "type": "string"
        }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}
